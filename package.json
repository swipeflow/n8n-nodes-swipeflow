{
  "name": "n8n-nodes-swipeflow",
  "version": "0.1.0",
  "description": "n8n community node for SwipeFlow",
  "keywords": [
    "n8n",
    "swipeflow",
    "swipeflow.io",
    "approval",
    "human approval",
    "human in the loop",
    "workflow automation",
    "wait for approval",
    "send and wait for response",
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://swipeflow.io",
  "author": {
    "name": "SwipeFlow Team",
    "email": "support@swipeflow.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/swipeflow/n8n-nodes-swipeflow.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/SwipeFlowApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/SwipeFlow/SwipeFlow.node.js",
      "dist/nodes/SwipeFlow/SwipeFlowTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
